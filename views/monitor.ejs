    <!-- For President Vote -->
    <script>
      const myVar = <%- JSON.stringify(pres_name); %>;
        const voteNo = <%- JSON.stringify(pres_vote); %>;
        var ctx = document.getElementById('barChart0');
        var myChart = new Chart(ctx, {
            type: 'bar',
            data: {
                labels: myVar,
                datasets: [{
                    data: voteNo,
                    backgroundColor: [
                        'rgba(255, 99, 132, 0.2)',
                        'rgba(54, 162, 235, 0.2)',
                    ],
                    borderColor: [
                        'rgba(255, 99, 132, 1)',
                        'rgba(54, 162, 235, 1)',
                    ],
                    borderWidth: 1
                }]
            },
            options: {
                scales: {
                    yAxes: [{
                        ticks: {
                            beginAtZero: true
                        }
                    }]
                }
            }
        });
    </script>

